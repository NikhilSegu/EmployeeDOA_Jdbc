package com.veltech.entity;
public class Employee{
 private int eid;
 private String ename;
 private double sal;
 
 public Employee(){
	
 }
 public Employee(int eid,String ename,double sal){
     this.eid=eid;
	 this.ename=ename;
	 this.sal=sal;
 }
 public void setName(String ename){this.ename=ename;}
 public void setId(int eid){this.eid=eid;}
 public void setSal(double sal){this.sal=sal;}
 
 public int getId(){return eid;}
 public String getName(){return ename;}
 public double getSal(){return sal;}
 
}
